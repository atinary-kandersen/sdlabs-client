{
  "name": "sdlabs2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup": "npm rum openapi & npm run update:icons",
    "vite": "vite",
    "dev": "concurrently \"vite\" \"npm run json-server\" \"npm run socket-mirror\" --kill-others",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:node": "npx vitest run --workspace=vitest.workspace.ts --project node",
    "test:browser": "vitest --workspace=vitest.workspace.ts --project node",
    "test:e2e": "npx playwright test --ui",
    "lint": "eslint . && prettier --check .",
    "format": "eslint --fix . && prettier --write .",
    "json-server": "npx json-server dev/json-server.json --port 3001",
    "socket-mirror": "node dev/socketMirror.js",
    "openapi": "npx openapi-typescript \"./dev/openapi.yaml\" --output \"./src/generated/openapi.d.ts\"",
    "update:icons": "npm update '@awesome.me/kit-3979d6528f'",
    "bundle-visualizer": "vite-bundle-visualizer -c vite.config.ts --sourcemap"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@mantine/core": "^8.0.0",
    "@mantine/dropzone": "^8.0.0",
    "@mantine/hooks": "^8.0.0",
    "classnames": "^2.5.1",
    "openapi-fetch-typescript": "^0.0.12",
    "openapi-react-query": "^0.3.1",
    "openapi-typescript-fetch": "^2.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^5.0.0",
    "react-router": "^7.5.0",
    "socket.io-client": "^4.8.1",
    "valibot": "^1.0.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@tanstack/react-query-devtools": "^5.74.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser": "^3.1.3",
    "concurrently": "^9.1.2",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "json-server": "^1.0.0-beta.3",
    "openapi-typescript": "^7.6.1",
    "playwright": "^1.52.0",
    "prettier": "^3.5.3",
    "socket.io": "^4.8.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.0",
    "vite-bundle-visualizer": "^1.2.1",
    "vitest": "^3.1.3",
    "vitest-browser-react": "^0.1.1"
  }
}
