{
	"name": "sdlabs2",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"setup": "concurrently \"npm run openapi\" \"npm run json-server:generate\"",
		"vite": "vite",
		"dev": "concurrently \"npm run json-server:run\" \"npm run socket-mirror\" \"vite\" --kill-others",
		"build": "tsc -b && vite build",
		"preview": "concurrently \"npm run json-server:run\" \"npm run socket-mirror\" \"vite preview\" --kill-others",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:node": "npx vitest run --workspace=vitest.workspace.ts --project node",
		"test:browser": "vitest --workspace=vitest.workspace.ts --project node",
		"test:e2e": "npx playwright test --ui",
		"lint": "eslint . && prettier --check .",
		"format": "eslint --fix . && prettier --write .",
		"json-server:generate": "node --experimental-strip-types --disable-warning=ExperimentalWarning scripts/generateJsonServer.ts",
		"json-server:run": "npx json-server src/generated/json-server.json --port 3001",
		"socket-mirror": "node --experimental-strip-types scripts/socketMirror.ts",
		"openapi": "npx openapi-typescript \"./dev/openapi.yaml\" --output \"./src/generated/openapi.d.ts\"",
		"bundle-visualizer": "vite-bundle-visualizer -c vite.config.ts --sourcemap",
		"pre-commit": "npm run format"
	},
	"dependencies": {
		"@mantine/charts": "^8.0.0",
		"@mantine/core": "^8.0.0",
		"@mantine/dropzone": "^8.0.0",
		"@mantine/hooks": "^8.0.0",
		"classnames": "^2.5.1",
		"openapi-fetch": "^0.14.0",
		"openapi-react-query": "^0.5.0",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-error-boundary": "^5.0.0",
		"react-router": "^7.5.0",
		"socket.io-client": "^4.8.1",
		"valibot": "^1.0.0",
		"zustand": "^5.0.3"
	},
	"devDependencies": {
		"@eslint/js": "^9.24.0",
		"@faker-js/faker": "^9.7.0",
		"@tanstack/react-query-devtools": "^5.74.3",
		"@types/react": "^19.1.2",
		"@types/react-dom": "^19.1.3",
		"@vitejs/plugin-react": "^4.3.4",
		"@vitest/browser": "^3.1.3",
		"babel-plugin-react-compiler": "^19.1.0-rc.1",
		"concurrently": "^9.1.2",
		"eslint": "^9.24.0",
		"eslint-config-prettier": "^10.1.2",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.19",
		"globals": "^16.0.0",
		"json-server": "^1.0.0-beta.3",
		"openapi-typescript": "^7.8.0",
		"playwright": "^1.52.0",
		"prettier": "^3.5.3",
		"socket.io": "^4.8.1",
		"typescript": "~5.7.2",
		"typescript-eslint": "^8.30.1",
		"vite": "^6.3.0",
		"vite-bundle-visualizer": "^1.2.1",
		"vitest": "^3.1.3",
		"vitest-browser-react": "^0.1.1"
	}
}
